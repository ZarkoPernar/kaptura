{
    "presets": ["react"],
    "env": {
        "production": {
            "modules": false,
            "useBuiltIns": true,
            "targets": {
                "browsers": [
                    "Chrome >=60",
                    "Safari >=10.1",
                    "iOS >=10.3",
                    "Firefox >=54",
                    "Edge >=15"
                ]
            },
            "presets": ["minify"]
        },
        "test": {
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    },
    "plugins": [
        "lodash",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "transform-class-properties",
        "syntax-dynamic-import",
        "react-hot-loader/babel",
        ["transform-react-jsx"]
    ]
}
