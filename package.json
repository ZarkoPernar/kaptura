{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "My react, redux, and webpack boilerplate",
  "main": "server/index.js",
  "engines": {
    "node": "8.5.0"
  },
  "types": "server/index",
  "scripts": {
    "start": "node server",
    "build": "rimraf ./public && SET NODE_ENV=production && SET BABEL_ENV=production && webpack --config ./tools/webpack.config.js --colors --progress --profile",
    "test:mocha": "mocha --compilers js:babel-register -w test/testSetup.js src/**/*.spec.js",
    "test": "react-scripts test --env=jsdom",
    "serve": "SET NODE_ENV=production && nodemon -w server",
    "build:live": "nodemon --exec ts-node -- ./src/server/index.ts --config ./tsconfig.json",
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --open --config ./tools/webpack.config.js",
    "development": "cross-env NODE_ENV=development webpack -w ./src/web --config ./tools/webpack.config.js",
    "dev": "SET NODE_ENV=development && npm-run-all --parallel mongo build:live devserver",
    "mongo": "mongod",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
