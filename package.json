{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "My react, redux, and webpack boilerplate",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "test:mocha": "mocha --compilers js:babel-register -w test/testSetup.js src/**/*.spec.js",
    "test": "jest --watch",
    "serve": "nodemon -w server",
    "build:live": "nodemon --exec ts-node -- ./server/index.ts",
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --config ./tools/webpack.config.js",
    "dev": "npm-run-all --parallel build:live devserver mongo",
    "mongo": "mongod",
    "build": "rimraf ./public && SET NODE_ENV=production && webpack --config ./tools/webpack.config.js -p --colors --profile --progress",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^react(.*)$": "<rootDir>/node_modules/react-master$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tools/styleMock.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@types/node": "^7.0.18",
    "babel-core": "^6.23.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-minify-es2015": "0.0.3",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "babili": "0.0.12",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.8",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.2",
    "style-ext-html-webpack-plugin": "^3.4.0",
    "style-loader": "^0.13.2",
    "sw-precache-webpack-plugin": "^0.9.0",
    "typescript": "^2.3.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "inferno": "^3.1.1",
    "inferno-compat": "^3.1.1",
    "inferno-redux": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.9.8",
    "normalize.css": "5.0.0",
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "preact-redux": "2.0.1",
    "pushstate-server": "^3.0.0",
    "react": "^15.5.4",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-router-dom": "^4.1.1",
    "react-skylight": "^0.4.1",
    "redux": "3.6.0",
    "uuid": "^3.0.1"
  }
}
