    "types": "server/index",
    "scripts": {
        "start": "node server",
        "build": "rimraf ./public && SET NODE_ENV=production && SET BABEL_ENV=production && webpack --config ./tools/webpack.config.js --colors --progress --profile",
        "test": "react-scripts test --env=jsdom",
        "serve": "SET NODE_ENV=production && nodemon -w server",
        "build:live": "nodemon --exec ts-node -- ./src/server/index.ts --config ./tsconfig.json",
        "devserver": "cross-env NODE_ENV=development webpack-dev-server --config ./tools/webpack.config.js",
        "development": "cross-env NODE_ENV=development webpack -w ./src/web --config ./tools/webpack.config.js",
        "dev": "SET NODE_ENV=development && npm-run-all --parallel build:live devserver"
    },
